<!DOCTYPE html>
<html>

<head>
    <title>
        <%= title %>
    </title>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.5/angular.min.js" />
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
    <div ng-app="OsO" ng-controller="Ctrl">

        <h1>Devices:</h1>
        <p>{{devices}}</p>

    </div>

    <script>
        var app = angular.module('OsO', []);
        app.controller('Ctrl', function($scope, $http) {
            $http.get("devices/").then(function(response) {
                $scope.devices = response.data;
            }, function error(e) {
                console.log(e);
            });
        });
    </script>
</body>

</html>
